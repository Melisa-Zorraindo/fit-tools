{
  "name": "fit-tools",
  "version": "1.0.2",
  "description": "A lightweight TypeScript library to support weightlifting and diet planning.",
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "watch": "tsdown --watch",
    "test": "tsx --test --experimental-test-coverage"
  },
  "devDependencies": {
    "tsdown": "^0.15.11",
    "tsx": "^4.21.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "calculator",
    "diet",
    "fitness",
    "powerlifting",
    "weight training",
    "weightlifting",
    "typescript"
  ],
  "author": "Melisa Zorraindo <melisa.zorraindo@gmail.com> (https://melisazor.com/)",
  "license": "Unlicense",
  "module": "./dist/index.js",
  "homepage": "https://github.com/Melisa-Zorraindo/fit-tools/blob/main/README.md",
  "bugs": {
    "url": "https://github.com/Melisa-Zorraindo/fit-tools/issues",
    "email": "melisa.zorraindo@gmail.com"
  },
  "repository": {
    "repository": {
      "type": "git",
      "url": "https://github.com/Melisa-Zorraindo/fit-tools"
    }
  }
}
